<body>
    {{#if admins}}
    {{>Admin-Header}}
    {{else}}
    {{>User-Header}}
    {{/if}}

    {{#if shop}}
    <div class="shop_sidebar_area" style="position:relative;">

        <!-- ##### Single Widget ##### -->
        <div class="widget catagory mb-50">
            <!-- Widget Title -->
            <h6 class="widget-title mb-30">Catagories</h6>

            <!--  Catagories  -->
            <div class="catagories-menu">
                <ul>
                    {{#if shopMain}}
                    <li class="active"><a href="/shop">All Products</a></li>
                    {{else}}
                    <li><a href="/shop">All Products</a></li>
                    {{/if}}

                    {{#if shopChairs}}
                     <li class="active"><a href="/shop-chairs">Chairs</a></li>
                    {{else}}
                    <li ><a href="/shop-chairs">Chairs</a></li>
                    {{/if}}

                    {{#if shopBeds}}
                    <li class="active"><a href="/shop-beds">Beds</a></li>
                    {{else}}
                    <li><a href="/shop-beds">Beds</a></li>
                    {{/if}}
                    
                    {{#if shopDeco}}
                    <li class="active"><a href="/shop-home-deco">Home Deco</a></li>
                    {{else}}
                    <li><a href="/shop-home-deco">Home Deco</a></li>
                    {{/if}}
                    
                    {{#if shopDressing}}
                    <li class="active"><a href="/shop-dressing">Dressings</a></li>
                    {{else}}
                    <li><a href="/shop-dressing">Dressings</a></li>
                    {{/if}}
                    
                       {{#if shopTables}}
                    <li class="active"><a href="/shop-tables">Tables</a></li>
                    {{else}}
                       <li><a href="/shop-tables">Tables</a></li>
                    {{/if}}
                    
                 
                </ul>
            </div>
        </div>       
    </div>

    <div class="amado_product_area section-padding-100">
        <div class="container-fluid">
            <div class="row">

                {{{body}}}
            </div>
        </div>
    </div>

    {{else}}
    {{{body}}}
    {{/if}}
    
    {{#if footer}}
    <!-- ##### Footer Area Start ##### -->
